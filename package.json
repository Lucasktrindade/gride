{
  "name": "gride",
  "version": "0.3.1",
  "description": "Gride is a stylus and scss, simple grid system.",
  "main": "./index.js",
  "scripts": {
    "test": "tape -r babel-register test/*.js",
    "test:watch": "tape-watch -r babel-register test/*.js -p faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guuibayer/gride.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": "inline"
  },
  "keywords": [
    "grid",
    "stylus",
    "utility"
  ],
  "author": "guuibayer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guuibayer/gride/issues"
  },
  "homepage": "https://github.com/guuibayer/gride#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "css-parse": "^2.0.0",
    "faucet": "0.0.1",
    "stylus": "^0.54.5",
    "tape": "^4.6.3",
    "tape-watch": "^2.2.4"
  }
}
